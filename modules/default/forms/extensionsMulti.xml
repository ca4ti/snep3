<?xml version="1.0" encoding="UTF-8"?>

<form>
    <extension>
        <elements>
            <exten>
                <type>Text</type>
                <options>
                    <label>Extension</label>
                    <description>
                        Ex: 1000-1050;1060;1063;1070-1100
                    </description>
                    <validators>
                        <regex>
                            <validator>regex</validator>
                            <options>
                                <pattern>/^(([0-9]\-?\;?)+)$/</pattern>
                                <messages>
                                    <regexNotMatch>This field should contain numbers only</regexNotMatch>
                                </messages>
                            </options>
                        </regex>
                        <strlen>
                            <validator>StringLength</validator>
                            <options>
                                <min>7</min>
                            </options>
                        </strlen>
                    </validators>
                </options>
            </exten>
            <exten_group>
                <type>ExtensionGroup</type>
                <options>
                    <label>Extension Group</label>
                    <value>users</value>
                </options>
            </exten_group>
            <pickup_group>
                <type>PickupGroup</type>
                <options>
                    <label>Pickup Group</label>
                </options>
            </pickup_group>
        </elements>
    </extension>
    <technology>
        <elements>
            <type>
                <type>Select</type>
                <options>
                    <label>Type</label>
                    <multioptions>
                        <sip>SIP</sip>
                        <iax2>IAX2</iax2>
                        <khomp>Khomp</khomp>
                        <virtual>Virtual</virtual>
                    </multioptions>
                </options>
            </type>
        </elements>
    </technology>
    <ip>
        <elements>
            <nat>
                <type>MultiCheckbox</type>
                <options>
                    <label>NAT</label>
                    <multioptions>
                        <enable>Enable NAT</enable>
                    </multioptions>
                </options>
            </nat>
            <qualify>
                <type>MultiCheckbox</type>
                <options>
                    <label>Qualify</label>
                    <multioptions>
                        <option>Enable delay qualification</option>
                    </multioptions>
                </options>
            </qualify>
            <directmedia>
                <type>radio</type>
                <options>
                    <label>Direct Media</label>
                    <value>yes</value>
                    <separator> </separator>
                    <multioptions>
                        <yes>yes</yes>
                        <no>no</no>
                    </multioptions>
                </options>
            </directmedia>
            <dtmf>
                <type>radio</type>
                <options>
                    <label>DTMF Mode</label>
                    <value>rfc2833</value>
                    <separator> </separator>
                    <multioptions>
                        <rfc2833>RFC-2833</rfc2833>
                        <inband>Inband</inband>
                        <info>Info</info>
                    </multioptions>
                </options>
            </dtmf>
            <codec>
                <type>codec</type>
                <options>
                    <value>ulaw</value>
                    <label>Codec 1</label>
                    <description>Prioritary</description>
                </options>
            </codec>
            <codec1>
                <type>codec</type>
                <options>
                    <value>alaw</value>
                    <label>Codec 2</label>
                </options>
            </codec1>
            <codec2>
                <type>codec</type>
                <options>
                    <value>ilbc</value>
                    <label>Codec 3</label>
                </options>
            </codec2>
        </elements>
    </ip>
    <virtual>
        <elements>
            <virtual>
                <type>Trunk</type>
                <options>
                    <label>Trunk</label>
                </options>
            </virtual>
        </elements>
    </virtual>
    <khomp>
        <elements>
            <board>
                <type>Select</type>
                <options>
                    <label>Board</label>
                </options>
            </board>
        </elements>
    </khomp>
</form>